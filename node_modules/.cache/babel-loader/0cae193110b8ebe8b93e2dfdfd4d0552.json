ted: {\r\n    showPhoto() {\r\n      if (this.cityDetails.regionID !== \"\")\r\n        return (\r\n          \"https://www.countryflags.io/\" +\r\n          this.cityDetails.regionID +\r\n          \"/shiny/64.png\"\r\n        );\r\n      else return \"\";\r\n    },\r\n    getFavorites() {\r\n      return this.$store.getters.favorites;\r\n    },\r\n    searchedValue: {\r\n      get() {},\r\n      set(evt) {\r\n        this.$store.dispatch(\"userSearch\", evt);\r\n      }\r\n    },\r\n    showCity() {\r\n      //this.cityDetails = this.$store.getters.cityDetails;\r\n      return this.cityDetails.city;\r\n    },\r\n    showTempC() {\r\n      return this.cityDetails.tempC + ' 째C';\r\n    },\r\n    showTempF() {\r\n      return this.cityDetails.tempF + ' 째F';\r\n    },\r\n    showWheaterText() {\r\n      return this.cityDetails.WeatherText;\r\n    }\r\n  },\r\n  created() {\r\n    this.cityDetails = this.$store.getters.cityDetails;\r\n  },\r\n};\r\n</script>\r\n<style >\r\n.customcontainer {\r\n  border-style: solid;\r\n}\r\n.city {\r\n  font-family: times, Times New Roman, times-roman, georgia, serif;\r\n  color: #444;\r\n  margin-top: 14px;\r\n}\r\n.favorite{\r\n  margin-top: 30px;\r\n}\r\n.weatherTXT{\r\n  font-size: 150px;\r\n\r\n}\r\n.text {\r\n    position: absolute;\r\n    top: 50%; \r\n    right: 50%;\r\n    margin-top: 100px;\r\n    transform: translate(50%,-50%);\r\n    text-transform: uppercase;\r\n    font-family: verdana;\r\n    font-size: 12em;\r\n    font-weight: 700;\r\n    color: #f5f5f5;\r\n    text-shadow: 1px 1px 1px #919191,\r\n        1px 2px 1px #919191,\r\n        1px 3px 1px #919191,\r\n        1px 4px 1px #919191,\r\n        1px 5px 1px #919191,\r\n        1px 6px 1px #919191,\r\n        1px 7px 1px #919191,\r\n        1px 8px 1px #919191,\r\n        1px 9px 1px #919191,\r\n        1px 10px 1px #919191,\r\n    1px 18px 6px rgba(16,16,16,0.4),\r\n    1px 22px 10px rgba(16,16,16,0.2),\r\n    1px 25px 35px rgba(16,16,16,0.2),\r\n    1px 30px 60px